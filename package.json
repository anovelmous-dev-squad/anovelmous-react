{
  "name": "anovelmous-web",
  "version": "0.1.0",
  "description": "Front-End for Anovelmous",
  "repository": {
    "type": "git",
    "url": "https://github.com/anovelmous-dev-squad/anovelmous-web"
  },
  "license": "BSD-3-Clause",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "webpack",
    "start": "npm run dev",
    "dev": "node --harmony bin/webpack-dev-server",
    "dev:nw": "npm run dev -- --nw",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "npm run test:unit && npm run test:server",
    "test:unit": "node ./node_modules/karma/bin/karma start",
    "deploy": "npm run test:unit && npm run compile",
    "update-schema": "babel-node ./scripts/updateSchema.js"
  },
  "author": "Greg Ziegan <greg.ziegan@gmail.com> (http://gregziegan.com)",
  "dependencies": {
    "babel": "^5.8.23",
    "babel-relay-plugin": "^0.2.4",
    "express": "^4.13.3",
    "express-graphql": "^0.3.0",
    "graphql": "^0.4.4",
    "graphql-relay": "^0.3.2",
    "history": "^1.9.0",
    "immutable": "^3.7.4",
    "invariant": "^2.1.0",
    "moment": "^2.10.6",
    "radium": "^0.14.1",
    "react": "^0.14.0",
    "react-autocomplete": "^0.1.4",
    "react-dom": "^0.14.0",
    "react-inline-grid": "^0.5.2",
    "react-relay": "^0.3.1",
    "react-router": "1.0.0-rc1",
    "react-router-relay": "^0.5.0",
    "yargs": "^3.18.0"
  },
  "devDependencies": {
    "autoprefixer-core": "^5.2.1",
    "autoprefixer-loader": "^2.0.0",
    "babel-eslint": "^4.1.1",
